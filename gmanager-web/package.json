    "vue": "2",
    "vue-router": "3",
    "vuex": "3"
  },
  "devDependencies": {
    "@babel/core": "7",
    "@babel/register": "7",
    "@vue/cli-plugin-babel": "4",
    "@vue/cli-service": "4",
    "html-webpack-plugin": "4",
    "mockjs": "1",
    "runjs": "^4.3.2",
    "sass-loader": "^7.1.0",
    "script-ext-html-webpack-plugin": "2.1.3",
    "script-loader": "0.7.2",
    "serve-static": "^1",
    "svg-sprite-loader": "4",
    "svgo": "1",
    "vue-template-compiler": "2"
